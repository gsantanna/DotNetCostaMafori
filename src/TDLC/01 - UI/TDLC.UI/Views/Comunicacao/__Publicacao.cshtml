@model TDLC.UI.Areas.Admin.Models.ViewModels.PublicacaoViewmodel






<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js ie6 oldie" lang="pt-br"> <![endif]-->
<!--[if IE 7]>    <html class="no-js ie7 oldie" lang="pt-br"> <![endif]-->
<!--[if IE 8]>    <html class="no-js ie8 oldie" lang="pt-br"> <![endif]-->
<!--[if gt IE 8]><!-->
<html class="no-js" lang="pt-br">
 <!--<![endif]-->
<head>
   @Url.Content("~/Content/img/header/" + ViewBag.imgheader + "")
</head>
<body class="news" id="page-top" data-spy="scroll" data-target=".navbar-fixed-top">
    <!-- ==============================================
    Preloader
    =============================================== -->
    
    @ViewBag.Title

    <!-- ==============================================
    Header
    =============================================== -->
    <header id="header" style="background-image: url('@Html.Raw(Model.Titulo)');">
        
    @Html.Raw(Model.Conteudos[0].Conteudo)



        <div class="wrap">


            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <h2 class="title-default">
                            @Html.Raw(ConteudoInstitucionalHelper.GetText(Convert.ToString(Session["cultura"]), "TIT-PAG_" + ViewBag.Tipo))

                        </h2>
                    </div><!-- /.col-lg-5 -->
                </div><!-- /.row -->
            </div><!-- /.container -->

            </div>

</header><!-- /header -->
    <!-- ==============================================
    CONTEUDO
    =============================================== -->
    <section class="">
        <div class="container">
            <div class="row cont-btn">
                <div class="col-lg-12">
                    <a href="javascript: history.go(-1)" class="btn btn-voltar pull-left">
                        <span class="ico type01" aria-hidden="true"></span> voltar
                    </a>
                </div><!-- /col -->
            </div><!-- /row -->
            <article>
                <div class="row">
                    <div class="col-md-5">
                        <span class="data">
                            @Model.DataPublicacao.ToString("dd/MM/yyyy")
                        </span>
                        <h2 class="title-cat">
                            @Model.Conteudos[0].Titulo
                         </h2>
                    </div><!-- /col -->
                    <div class="col-md-7">
                        <p>
                            @Html.Raw(Model.Conteudos[0].Conteudo )
                        </p>
                     
                    </div><!-- /col -->

                </div><!-- /row -->
            </article>
            <div class="row cont-btn">
                <div class="col-lg-12">
                    <a href="javascript: history.go(-1)" class="btn btn-voltar pull-left">
                        <span class="ico type01" aria-hidden="true"></span> voltar
                    </a>
                </div><!-- /col -->
            </div><!-- /row -->
        </div><!-- /container -->
    </section><!-- / -->
    <!-- ==============================================
    Footer
    =============================================== -->
    @Html.Partial("_footer")

</body>
</html>

